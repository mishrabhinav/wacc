{
  class A {
    int a;
    int b;

    void init(int x, int y) {
      @a = x ;
      @b = y
    }

    # Setter for member
    void a(int x) {
      @a = x
    }

    # Getter for member
    int a() {
      return @a
    }

    void foo() {
      println "bar"
    }
  }

  # Initiate new class
  A a = new A() ;

  # Constructor
  call a->init(2, 3) ;

  # Calling function
  call a->foo() ;

  # Get a member
  int y = call a->b() ;

  # Set a member
  call a->a(y)
}
