{
  class A {
    int a {GET, SET};
    int b;

    void init(int x, int y) {
      @a = x ;
      @b = y
    }

    # Setter for member b
    void b(int x) {
      @b = x
    }

    # Getter for member b
    int b() {
      return @b
    }

    void foo() {
      println "bar"
    }
  }

  # Initiate new class
  A a = new A(2, 3) ;

  # Calling function
  call a->foo() ;

  # Get a member
  int y = call a->b() ;
  println y;

  # Set a member
  call a->a(y)
}
